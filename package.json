{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "Storefront using Express and Postgres",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "npx tsc",
    "watch": "tsc-watch --onSuccess 'node ./dist/index.js'",
    "nodemon": "nodemon src/index.ts",
    "config": "node -r dotenv/config ./dist/database.js",
    "migrate": "db-migrate --env dev up",
    "test": "ENV=test db-migrate --env test up && jasmine-ts && db-migrate db:drop test"
  },
  "author": "Gabriel Ramirez",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.12",
    "db-migrate-pg": "^1.2.2",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "typescript": "^4.3.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.11",
    "@types/express": "^4.17.12",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/node": "^15.12.4",
    "@types/pg": "^8.6.0",
    "dotenv": "^10.0.0",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.3.0",
    "nodemon": "^2.0.11",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genrique98/SQL-Express-Storefront.git"
  },
  "bugs": {
    "url": "https://github.com/genrique98/SQL-Express-Storefront/issues"
  },
  "homepage": "https://github.com/genrique98/SQL-Express-Storefront#readme"
}
